<style>
    @keyframes show {
        from {
            transform: translateY(100%);
            opacity: 0;
        }
        to {
            transform: translateY(0);
            opacity: 1;
        }
    }
    .skill-card {
        view-timeline-name: --reveal;
        animation-name: show;
        animation-fill-mode: both;
        animation-timeline: --reveal;
        animation-range: entry 25% cover 35%;
    }
</style>
<script type="module">
    class Particle {
        constructor(x, y, size, color) {
            this.x = x;
            this.y = y;
            this.size = size;
            this.baseX = x;
            this.baseY = y;
            this.density = Math.random() * 30 + 1;
            this.color = color;
            this.isActive = false;
            this.vx = (Math.random() - 0.5) * 3;
            this.vy = (Math.random() - 0.5) * 3;
        }

        activate() {
            this.isActive = true;
        }

        reset() {
            this.x = this.baseX;
            this.y = this.baseY;
            this.isActive = false;
        }

        update() {
            this.x += this.vx;
            this.y += this.vy;
        }

        draw(ctx) {
            ctx.fillStyle = this.color;
            ctx.fillRect(this.x, this.y, this.size, this.size);
        }
    }

    class PixelCanvas {
        constructor(canvas, card) {
            this.canvas = canvas;
            this.card = card;
            this.ctx = canvas.getContext("2d");
            this.particles = [];
            this.isAnimating = false;
            this.animationFrame = null;
            this.pixelSize = 4;
            this.color = card.dataset.effectColor || "#61dafb";

            this.init();
            this.addEventListeners();
        }

        init() {
            this.resizeCanvas();
            window.addEventListener("resize", () => this.resizeCanvas());
        }

        resizeCanvas() {
            const rect = this.card.getBoundingClientRect();
            this.canvas.width = rect.width;
            this.canvas.height = rect.height;

            this.createParticles();
        }

        createParticles() {
            this.particles = [];
            for (let y = 0; y < this.canvas.height; y += this.pixelSize) {
                for (let x = 0; x < this.canvas.width; x += this.pixelSize) {
                    this.particles.push(
                        new Particle(x, y, this.pixelSize, this.color),
                    );
                }
            }

            // Dibujar estado inicial
            this.ctx.fillStyle = this.color;
            this.ctx.fillRect(0, 0, this.canvas.width, this.canvas.height);
        }

        animate() {
            this.ctx.clearRect(0, 0, this.canvas.width, this.canvas.height);

            let isAnyParticleActive = false;
            this.particles.forEach((particle) => {
                if (particle.isActive) {
                    particle.update();
                    particle.draw(this.ctx);
                    isAnyParticleActive = true;
                } else {
                    particle.draw(this.ctx);
                }
            });

            if (isAnyParticleActive && this.isAnimating) {
                this.animationFrame = requestAnimationFrame(() =>
                    this.animate(),
                );
            } else {
                this.isAnimating = false;
            }
        }

        addEventListeners() {
            this.card.addEventListener("mouseenter", () => {
                this.isAnimating = true;
                this.particles.forEach((particle) => particle.activate());
                this.animate();
            });

            this.card.addEventListener("mouseleave", () => {
                this.particles.forEach((particle) => particle.reset());
            });
        }
    }

    // Inicializar el efecto en todas las cards
    document.querySelectorAll(".skill-card").forEach((card) => {
        const canvas = card.querySelector("canvas");
        new PixelCanvas(canvas, card);
    });
</script>
